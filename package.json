{
    "name": "wp-env-codespace",
    "dependencies": {
        "playwright": "^1.38.1"
    },
    "devDependencies": {
        "@wordpress/env": "^8.8.0",
        "@wordpress/eslint-plugin": "^15.0.0",
        "@wordpress/scripts": "^26.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "sass": "^1.60.0"
    },
    "scripts": {
        "test": "npx playwright test",
        "up": "WP_ENV_HOME=wp-env-home wp-env start --xdebug=profile,trace,debug,coverage",
        "wp-env": "WP_ENV_HOME=wp-env-home wp-env",
        "cli": "WP_ENV_HOME=wp-env-home wp-env run cli bash",
        "phpunit": "npm run wp-env run tests-cli ~/.composer/vendor/bin/phpunit -- -c wp-content/plugins/ionos-security/tests/phpunit/phpunit.xml",
        "test-coverage": "npx wp-env run tests-cli ~/.composer/vendor/bin/phpunit -- -c wp-content/plugins/ionos-security/tests/phpunit/phpunit.xml --coverage-html wp-content/plugins/ionos-security/tests/phpunit/reports/"
    },
    "config": {}
}
